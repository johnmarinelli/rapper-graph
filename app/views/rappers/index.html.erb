<h1>Listing rappers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Year</th>
      <th>Influenced</th>
      <th>Beef</th>
      <th>Collaborated</th>
	  <th>Image Path</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rappers.each do |rapper| %>
      <tr>
        <td><%= rapper.name %></td>
        <td><%= rapper.year %></td>
        <td><%= rapper.influenced %></td>
        <td><%= rapper.beef %></td>
        <td><%= rapper.collaborated %></td>
		<td><%= rapper.image_path %></td>
        <td><%= link_to 'Show', rapper %></td>
        <td><%= link_to 'Edit', edit_rapper_path(rapper) %></td>
        <td><%= link_to 'Destroy', rapper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rapper', new_rapper_path %>
